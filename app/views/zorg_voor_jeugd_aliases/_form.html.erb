<% semantic_form_for(@zorg_voor_jeugd_alias) do |f| %>
  <%= f.error_messages %>

  <% f.inputs "Gebruiker in Antorcha" do %>
    <%= f.input :organization_id, :collection => Organization.all.collect {|x| [x.title, x.id]} %>
    <%= f.input :organization_username %>
  <% end %>
  <% f.inputs "Gebruiker in Zorg voor Jeugd" do %>
    <%= f.input :organisatie_uuid %>
    <%= f.input :organisatie_naam %>
    <%= f.input :organisatie_postcode %>
    <%= f.input :gebruikersnaam_medewerker %>
  <% end %>
  <% f.buttons do %>
    <%= f.commit_button %> of <%= link_to 'ga terug naar het overzicht', zorg_voor_jeugd_aliases_path %>
  <% end %>

<% end %>